<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE hibernate-mapping PUBLIC
	"-//HIbernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="DataAccess.Colours" table="colours" >
	
		<id name="ColourID" >
			<column name="ColourID" length="10" not-null="true" />
			<generator class="assigned" />
		</id>
		
		<property name="ColourName" >
			<column name="ColourName" length="10" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
			
			