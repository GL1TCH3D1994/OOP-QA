<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE hibernate-mapping PUBLIC
	"-//HIbernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="DataAccess.OrderItems" table="orderItems" >
	
		<id name="OrderItemNumber" >
			<column name="OrderItemNumber" length="10" not-null="true" />
			<generator class="assigned" />
		</id>
		
		<property name="OrderNumber" >
			<column name="OrderNumber" length="10" not-null="true" />
		</property>
		
		<property name="ProductNumber" >
			<column name="ProductNumber" length="10" not-null="true" />
		</property>
		
		<property name="PurchasePrice" >
			<column name="PurchasePrice" length="10" not-null="true" />
		</property>
		
		<property name="Quantity" >
			<column name="Quantity" length="10" not-null="true" />
		</property>
		
		<property name="Length" >
			<column name="Length" length="10" not-null="true" />
		</property>
		
		<property name="ColourID" >
			<column name="ColourID" length="10" not-null="true" />
		</property>
		
		<property name="ApplySeasonalDiscount" >
			<column name="ApplySeasonalDiscount" length="10" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
			
			